[0.063464, "o", "ed \"0,2\"\r\n\r\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\r\n\r\ncode - Syntax-highlighted code viewer\r\n\r\n  Usage: termos run --title \"<text>\" code --file <value>\r\n\r\n  Options:\r\n    --file         Path to source file (required)\r\n    --highlight    Line range to highlight (e.g. '10-20')\r\n    --line         Scroll to line number\r\n\r\n  Returns:\r\n    action: accept\r\n    file: string - path to file\r\n\r\n  Examples:\r\n    termos run --title \"Code\" code --file src/index.ts\r\n    termos run --title \"Code\" code --file src/app.tsx --highlight \"15-25\" --line 15\r\n\r\n━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━\r\n\r\ndiff - Show file changes (git diff"]
